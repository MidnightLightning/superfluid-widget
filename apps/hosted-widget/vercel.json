{
  "rewrites": [
    { "source": "/:hash", "destination": "/[hash]" },
    { "source": "/demo/:hash", "destination": "/demo/[[...hash]]" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://app.safe.global" }
      ]
    }
  ]
}
